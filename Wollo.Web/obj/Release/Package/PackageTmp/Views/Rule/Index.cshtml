<div class="modal fade" id="administrator" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="administratorLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">@Html.LabelFor(x => x.AdminRuleMasterData.administration_fee)</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="dropdown col-sm-3">
                        <p class="text-left"> @Html.LabelFor(x => x.AdminRuleMasterData.select_stock) </p>
                    </div>
                    <div class="dropdown col-sm-3">
                        @{string select_stock_code_label = @Html.DisplayNameFor(model => model.AdminRuleMasterData.select_stock_code).ToString();}
                        @Html.DropDownList("AdminStock", new SelectList(Model.AdminRuleMasterData.StockCodes, "id", "stock_name"), select_stock_code_label, new { @class = "form-control" })
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="clearfix"></div>
                    <div class="dropdown col-sm-3">
                        <p class="text-left">@Html.LabelFor(x => x.AdminRuleMasterData.administration_fee_on_buyer) </p>
                    </div>
                    <div class="dropdown col-sm-3">
                        @{string select_rule_label = @Html.DisplayNameFor(model => model.AdminRuleMasterData.select_rule).ToString();}
                        @Html.DropDownList("FeeRuleBuyer", new SelectList(Model.AdminRuleMasterData.RuleConfigSettings, "rule_id", "RuleConfigMaster.rule_name"), select_rule_label, new { @class = "form-control" })
                    </div>
                    <div class="dropdown col-sm-3">
                        @Html.TextBox("FeeBuyer", null, new { @class = "form-control", @placeholder = @Html.DisplayNameFor(model => model.AdminRuleMasterData.buyer_fee) })
                    </div>
                    <div class="clearfix"></div>
                    @*<br />*@
                    <div class="clearfix"></div>
                    <div class="dropdown col-sm-3">
                        <p class="text-left">@Html.LabelFor(x => x.AdminRuleMasterData.administration_fees_on_seller) </p>
                    </div>
                    <div class="dropdown col-sm-3">
                        @Html.DropDownList("FeeRuleSeller", new SelectList(Model.AdminRuleMasterData.RuleConfigSettings, "rule_id", "RuleConfigMaster.rule_name"), select_rule_label, new { @class = "form-control" })
                    </div>
                    <div class="dropdown col-sm-3">
                        @Html.TextBox("FeeSeller", null, new { @class = "form-control", @placeholder = @Html.DisplayNameFor(model => model.AdminRuleMasterData.seller_fee) })
                    </div>
                    <div class="clearfix"></div>


                    <div class="dropdown col-sm-12">
                        <table class="table  table-striped table-bordered table-hovered" style="margin-top:5px;">
                            <caption><center>@Html.LabelFor(x => x.AdminRuleMasterData.current_administration_fees_rule)</center></caption>
                            @*<table class="table  table-striped">
                                <caption>Current Administration Fees Rule</caption>*@
                            <tr>
                                <th>@Html.LabelFor(x => x.AdminRuleMasterData.rule_on)</th>
                                <th>@Html.LabelFor(x => x.AdminRuleMasterData.stock)</th>
                                <th>@Html.LabelFor(x => x.AdminRuleMasterData.rule)</th>
                                <th>@Html.LabelFor(x => x.AdminRuleMasterData.fee)</th>
                                <th>@Html.LabelFor(x => x.AdminRuleMasterData.updated_on)</th>

                            </tr>
                            @foreach (var item in Model.AdminRuleMasterData.AdministrationFees)
                            {

                                <tr>
                                    <td>@Html.DisplayFor(y => item.apply_on)</td>
                                    <td>@Html.DisplayFor(x => item.StockCode.stock_code)</td>
                                    <td>@Html.DisplayFor(x => item.RuleConfigMaster.rule_name)</td>
                                    <td>@Html.DisplayFor(x => item.fees)</td>
                                    <td>
                                        @{string date = item.updated_date.Value.ToString("dd'/'MM'/'yyyy HH:mm:ss");}
                                        @Html.Raw(date)
                                        @*@Html.DisplayFor(x => item.updated_date.Value.GetDateTimeFormats()[3])*@
                                    </td>
                                </tr>
                            }
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">@Html.LabelFor(x => x.AdminRuleMasterData.cancel)</button>
                <button type="button" onclick="this.disabled=true" class="btn btn-primary" id="AddAdminRule">@Html.LabelFor(x => x.AdminRuleMasterData.ok)</button>
            </div>
        </div>
    </div>
</div>


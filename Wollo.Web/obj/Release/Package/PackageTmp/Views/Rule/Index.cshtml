@model Wollo.Web.Models.AdminRuleMasterDataModel
@{
    ViewBag.Title = @Html.DisplayNameFor(x => x.AdminRuleMasterData.rules);
}
<style>
    .table {
        width: 100%;
        max-width: 100%;
        margin-bottom: 20px;
    }

    .circle {
        position: relative;
        border-radius: 50%;
        width: 100%;
        height: auto;
        padding-top: 100%;
        background-image: url(/images/administrator.jpg);
        background-repeat: round;
        text-align: center;
    }

    .circle1 {
        position: relative;
        border-radius: 50%;
        width: 100%;
        height: auto;
        padding-top: 100%;
        background-image: url(/images/Withdraw.jpg);
        background-repeat: round;
        text-align: center;
    }

    .circle2 {
        position: relative;
        border-radius: 50%;
        width: 100%;
        height: auto;
        padding-top: 100%;
        background-image: url(/images/TradeRule.jpg);
        background-repeat: round;
        text-align: center;
    }

    .lblRuleAndlink {
        text-align: center;
    }

    .btn {
        //padding: 3px 60px;
    }
</style>
<div id="page-wrapper">
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    @Html.LabelFor(x => x.AdminRuleMasterData.rules)
                </h1>
                <ol class="breadcrumb">
                    <li class="active">
                        <i class="fa fa-dashboard"></i> @Html.LabelFor(x => x.AdminRuleMasterData.dashboard_rules)
                    </li>
                </ol>
            </div>
        </div>
        <br />
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-4 ">
                    <div class="col-md-2"></div>
                    <div class="col-md-8 lblRuleAndlink ">
                        <div class="circle">
                        </div>
                        <br />
                        @Html.LabelFor(x => x.AdminRuleMasterData.administration_fee)
                        <br /><br /><br /><br />
                        <input type="button" class="btn btn-block btn-info" value=@Html.DisplayNameFor(model => model.AdminRuleMasterData.select) id="admin-container" /><br />
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <div class="col-md-4 ">
                    <div class="col-md-2"></div>
                    <div class="col-md-8 lblRuleAndlink">
                        <div class="circle1">
                        </div>
                        <br />
                        @Html.LabelFor(x => x.AdminRuleMasterData.withdrawal_fees)
                        <br /><br /><br /><br />
                        <input type="button" class="btn btn-block btn-info" value=@Html.DisplayNameFor(model => model.AdminRuleMasterData.select) id="withdrawal-container" /><br />
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <div class="col-md-4 ">
                    <div class="col-md-2"></div>
                    <div class="col-md-8 lblRuleAndlink">
                        <div class="circle2">

                        </div>
                        <br />
                        @Html.LabelFor(x => x.AdminRuleMasterData.trading_rules)
                        <br /><br /><br /><br />
                        <input type="button" class="btn btn-block btn-info" value=@Html.DisplayNameFor(model => model.AdminRuleMasterData.select) id="trading-container" /><br />
                    </div>
                    <div class="col-md-2"></div>
                </div>
            </div>

        </div>

    </div>

    <!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->
<!-- /#Model Window -->


@model IEnumerable<Wollo.Entities.ViewModels.Transaction_History>

@{
    ViewBag.Title = "All Trading History";
}
<div id="page-wrapper">
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    @Html.LabelFor(model=>model.FirstOrDefault().all_trading_history)
                </h1>
                <ol class="breadcrumb">
                    <li class="active">
                        <i class="fa fa-dashboard"></i> @Html.LabelFor(model => model.FirstOrDefault().dashboard_all_trading_history)
                    </li>
                </ol>
            </div>
        </div>
        <div class="container-fluid">

            <div class="col-lg-12">
                <div id="upper-birth">

                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped">
                            <thead>
                                <tr>
                                    <th class="no-b-th"> <div class="text-left">@Html.LabelFor(model => model.FirstOrDefault().trading_history)</div></th>
                                    <th class="empty-th"> </th>
                                    <th class="empty-th"> </th>
                                    <th class="empty-th"> </th>
                                    <th class="empty-th"> </th>
                                    <th class="empty-th"> </th>
                                    <th class="empty-th"> </th>
                                    <th class="empty-th"> </th>
                                    @*<th class="no-b-th">
                                            <div class="text-right">
                                                <div class="dropdown">
                                                    <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu11" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                                        Open
                                                        <span class="caret"></span>
                                                    </button>
                                                    <ul class="dropdown-menu" aria-labelledby="dropdownMenu11">
                                                        <li><a href="#">Open</a></li>
                                                        <li><a href="#">Deal</a></li>
                                                        <li><a href="#">Cancelled</a></li>
                                                        <li><a href="#">Partial</a></li>
                                                        <li><a href="#">All</a></li>
                                                    </ul>
                                                </div>

                                            </div>
                                        </th>*@
                                </tr>
                                <tr>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().StockCode.stock_code)</th>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().created_date)</th>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().id)</th>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().StockCode.stock_name)</th>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().original_order_quantity)</th>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().price)</th>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().queue_action)</th>
                                    <th>@Html.LabelFor(model => model.FirstOrDefault().QueueStatusMaster.name)</th>
                                    @*<th>Seller ID</th>
                                        <th>Seller Price</th>*@
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(model => item.StockCode.stock_code)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.created_date)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.id)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.StockCode.stock_name)
                                        </td>
                                        @if (item.QueueStatusMaster.name == "Queued")
                                        {
                                            <td>
                                                @Html.DisplayFor(model => item.reward_points)
                                            </td>
                                        }
                                        else
                                        {
                                            <td>
                                                @Html.DisplayFor(model => item.original_order_quantity)
                                            </td>
                                        }
                                        <td>
                                            @Html.DisplayFor(model => item.price)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.queue_action)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.QueueStatusMaster.name)
                                        </td>
                                        @*<td>
                                                @Html.DisplayFor(model => item.price)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(model => item.price)
                                            </td>*@
                                    </tr>
                                }
                            </tbody>
                        </table>

                        @*<nav class="text-center">
                                <ul class="pagination">
                                    <li class="disabled"><a href="#" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a></li>
                                    <li class="active"><a href="#">1 <span class="sr-only">(current)</span></a></li>
                                    <li><a href="#">2</a></li>
                                    <li><a href="#">3</a></li>
                                    <li><a href="#">4</a></li>
                                    <li><a href="#">5</a></li>
                                    <li>
                                        <a href="#" aria-label="Next">
                                            <span aria-hidden="true">&raquo;</span>
                                        </a>
                                    </li>
                                </ul>
                            </nav>*@
                    </div>
                </div>
            </div>
            <!-- /.container-fluid -->
        </div>
    </div>
</div>


@section scripts{
    <script>
        $("#nav-trading").addClass("active");
    </script>
}